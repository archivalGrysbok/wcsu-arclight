<!--************************************************************-->

<!--************************************************************-->
<!--          CONTENT MODEL COMPONENTS                          -->
<!--************************************************************-->

<!--************************************************************-->
<!--          BASIC COMPONENTS                                  -->

<!--          RENDER ELEMENTS                                   -->

<!ENTITY % m.render             'emph | lb'                       >

<!ENTITY % m.ptrs               'ptr | extptr'                    >

<!ENTITY % m.refs               'ref | extref | linkgrp |
                                 bibref | title | archref'        >

<!ENTITY % m.access             'corpname | famname | geogname |
                                 name | occupation | persname |
                                 subject | genreform | function'  >

<!ENTITY % m.data               '%m.access; | date | num |
                                 origination | repository |
                                 unitdate | unittitle'            >

<!--************************************************************-->
<!--     PHRASE COMPONENT                                       -->
<!--************************************************************-->
<!-- Phrase component models are widely used groups of elements.
     The models are defined progessively, the models progress from
     the least complex to the most complex.                     -->

<!--     BARE PHRASE                                            -->

<!ENTITY % m.phrase.bare        '%m.ptrs; | %m.render;'           >


<!--     BASIC PHRASE                                           -->


<!-- m.phrase.basic.norefs is used in content model of bibref
     and archref, and in the model used by ref and archref,
     where inclusion of ref elements is selective               -->

<!ENTITY % m.phrase.basic.norefs
                                '%m.phrase.bare; | abbr | expan'  >



<!ENTITY % m.phrase.basic       '%m.phrase.basic.norefs; |
                                 %m.refs;'                        >

<!--     PHRASE PLUS ELEMENTS                                   -->

<!-- m.phrase.plus.norefs used in content model of
     para.content.norefs which is used in content model of ref
     and archref, where inclusion of ref elements is selective. -->

<!ENTITY % m.phrase.plus.norefs '%m.phrase.basic.norefs; |
                                 %m.data;'                        >


<!ENTITY % m.phrase.plus        '%m.phrase.basic.norefs; |
                                 %m.data; | %m.refs;'             >



<!--************************************************************-->
<!--          INTERMEDIATE-LEVEL COMPONENTS                     -->
<!--************************************************************-->
<!-- Elements that can occur within or among paragraphs.        -->

<!ENTITY % m.inter.noquote      'address | chronlist | list |
                                 note | table'                    >

<!ENTITY % m.inter              '%m.inter.noquote; | blockquote'  >




<!--************************************************************-->
<!--          PARAGRAPH-LEVEL COMPONENTS                        -->
<!--************************************************************-->
<!-- Elements that can occur at the same level as a paragraph.  -->

<!ENTITY % m.blocks             '%m.inter; | p'                   >


<!-- m.block.noquote used in content model of blockquote        -->

<!ENTITY % m.blocks.noquote     '%m.inter.noquote; | p'           >


<!--************************************************************-->
<!--          COMPONENTS FOR DESCRIPTIVE ELEMENTS               -->
<!--************************************************************-->

<!--     DESCRIPTIVE IDENTIFICATION ELEMENTS                    -->

<!ENTITY % m.did                'abstract | physdesc | note |
                                 repository | origination |
                                 unitdate | unitid | unittitle |
                                 container | physloc |
                                 dao | daogrp'                    >

<!--     DESCRIPTIVE ELEMENTS                                   -->

<!-- m.desc.base used in dentry                                 -->

<!ENTITY % m.desc.base          'admininfo | bioghist |
                                 controlaccess | odd |
                                 scopecontent | organization |
                                 arrangement | add'               >


<!ENTITY % m.desc.full          '%m.desc.base; | dsc |
                                 dao | daogrp | note'             >


<!--     ADMINISTRATIVE INFORMATION                             -->

<!ENTITY % m.admininfo          'accessrestrict | acqinfo |
                                 altformavail | appraisal |
                                 custodhist | prefercite |
                                 processinfo | userestrict |
                                 accruals'                        >

<![%tabular;[

<!--     DISPLAY ENTRY                                          -->

<!ENTITY % m.dentry             '%m.did; | %m.desc.base;'         >

]]>

<!--************************************************************-->
<!--          INSIDE PARAGRAPHS AND NOTES                       -->
<!--************************************************************-->

<!--                             PARAGRAPH CONTENT
                                 Elements that can occur inside a
                                 paragraph
-->
<!ENTITY % m.para.content       '%m.phrase.plus; | %m.inter;'     >

<!-- para.content.norefs used in content model of ref elements,
     where inclusion of ref elements is selective               -->

<!ENTITY % m.para.content.norefs
                                '%m.phrase.plus.norefs; |
                                 %m.inter;'                       >

<!--************************************************************-->
<!--          INSIDE TABLE ENTRY                                -->
<!--************************************************************-->

<!ENTITY % m.table.entry        '%m.phrase.plus; | address |
                                 list | note'                     >



<!--************************************************************-->
<!--          COMPLETE CONTENT MODELS                           -->
<!--************************************************************-->


<!--                             HEADS AND PARAGRAPHS           -->
<!ENTITY % cm.head.n.para       '(head?, (%m.blocks;)*)'          >



<!--************************************************************-->
<!--          PARAMETER ENTITIES FOR ATTRIBUTE LISTS            -->
<!--************************************************************-->

<!--************************************************************-->
<!--          PARAMETER ENTITIES FOR CLUSTERS OF VALUES         -->
<!--************************************************************-->

<!ENTITY % av.level             'series | collection | file |
                                 fonds | item | otherlevel |
                                 recordgrp |  subgrp | subseries' >

<!ENTITY % av.render            'bold | italic | bolditalic |
                                 boldunderline | boldsmcaps |
                                 boldquoted | nonproport |
                                 altrender | quoted | smcaps |
                                 sub | super | underline'         >

<!--     SOURCE ATTRIBUTE VALUES:

         aat      = Art and Architecture Thesaurus
         aacr2    = Anglo-American Cataloging rules, 2nd edition,
                     revised
         cdwa     = Categories for the Description of Works of Art
         dot      = Dictionary of Occupational Titles
         gmgpc    = Thesaurus for Graphic Materials II
         lcnaf    = Library of Congress Name Authority File
         lcsh     = Library of Congress Subject Headings
         lctgm    = Thesaurus for Graphic Materials I
         local    = heading from a locally maintained list of
                     index terms
         mesh     = Medical Subject Headings
         mim      = Moving Image Materials
         ncarules = National Council of Archives, Rules for the
                     Construction of Personal, Corporate and Place
                     Names
         nmc      = Revised Nomenclature for Museum Cataloging
         rad      = Rules for Archival Description
         rbgenr   = Genre Terms
         tgn      = Thesaurus of Geographic Names
         ulan     = Union List of Artist Names

-->

<!ENTITY % av.sources           'aat | aacr2 | cdwa | dot |
                                 gmgpc | lcnaf | lcsh | lctgm |
                                 local | mesh | mim | ncarules |
                                 nmc | rad | rbgenr | tgn | ulan |
                                 othersource'                     >

<!ENTITY % av.container         'carton | box | folder | reel |
                                 frame | oversize | reel-frame |
                                 volume | map-case | box-folder |
                                 page | folio | othertype'        >


<!--************************************************************-->
<!--          COMMON ATTRIBUTES                                 -->
<!--************************************************************-->
<!--  Attributes used on most ead elements                      -->

<!ENTITY % a.common
         'id                ID                       #IMPLIED
          altrender         CDATA                    #IMPLIED
          audience          (external | internal)    #IMPLIED'    >


<!--************************************************************-->
<!--          ACCESS ATTRIBUTES                                 -->
<!--************************************************************-->
<!-- Attributes shared by all access elements.                  -->

<!ENTITY % a.access
         'source             (%av.sources;)          #IMPLIED
          othersource        CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED
          authfilenumber     CDATA                   #IMPLIED
          normal             CDATA                   #IMPLIED'    >


<!--************************************************************-->
<!--          DESCRIPTION ATTRIBUTES                            -->
<!--************************************************************-->
<!-- Descriptive attributes shared by archdesc and c's.         -->

<!ENTITY % a.desc.base
         'otherlevel         CDATA                      #IMPLIED
          langmaterial       CDATA                      #IMPLIED
          legalstatus        (public | private |
                              otherlegalstatus)         #IMPLIED
          otherlegalstatus   CDATA                      #IMPLIED' >

<!-- Archdesc (add REQUIRED level attribute)                    -->

<!ENTITY % a.desc.all
         'level              (%av.level;)               #REQUIRED
          %a.desc.base;'                                          >

<!-- Components (add IMPLIED level attribute)                   -->

<!ENTITY % a.component
         'level              (%av.level;)               #IMPLIED
          %a.desc.base;'                                          >


<!--************************************************************-->
<!--************************************************************-->
<!--          LINKING AND POINTING ATTRIBUTES WITH XLINK        -->
<!--************************************************************-->
<!--************************************************************-->

<!--************************************************************-->
<!--          XLINK ATTRIBUTE SETS                              -->
<!--************************************************************-->

<![%xlink;[


<!ENTITY % simple
         'xlink:form          CDATA     #FIXED       "simple"'    >

<!ENTITY % extended
         'xlink:form          CDATA     #FIXED       "extended"'  >

<!ENTITY % locator
         'xlink:form          CDATA     #FIXED       "locator"'   >
]]>

<![%noxlink;[


<!ENTITY % simple
         ''                                                       >

<!ENTITY % extended
         ''                                                       >

<!ENTITY % locator
         ''                                                       >

]]>

<!ENTITY % locator.att
         'href                CDATA                  #IMPLIED'    >

<!ENTITY % link-semantics.att
         'inline              (true|false)           "true"
          role                CDATA                  #IMPLIED'    >

<!ENTITY % simple-link-semantics.att
         'inline              (true|false)           "true"'      >

<!ENTITY % remote-resource-semantics.att
         'role                CDATA                  #IMPLIED
          title               CDATA                  #IMPLIED
          show                (embed|replace|new)    #IMPLIED
          actuate             (auto|user)            #IMPLIED
          behavior            CDATA                  #IMPLIED'    >

<!ENTITY % local-resource-semantics.att
         'content-role        CDATA                  #IMPLIED
          content-title       CDATA                  #IMPLIED'    >


<!--************************************************************-->
<!--          END XLINK ATTRIBUTE SETS                          -->
<!--************************************************************-->

<!--************************************************************-->
<!--          EAD LINKING AND POINTING ATTRIBUTES               -->
<!--************************************************************-->

<!ENTITY % a.internal.ptr
         'target              IDREF                  #IMPLIED
          xpointer            CDATA                  #IMPLIED
          %simple;
          %locator.att;
          %remote-resource-semantics.att;
          %local-resource-semantics.att;
          %simple-link-semantics.att;'                            >



<!ENTITY % a.external.ptr
         'entityref           ENTITY                 #IMPLIED
          xpointer            CDATA                  #IMPLIED
          %simple;
          %locator.att;
          %remote-resource-semantics.att;
          %local-resource-semantics.att;
          %simple-link-semantics.att;'                            >


<!ENTITY % a.linkgrp
         '%extended;
          %link-semantics.att;
          %local-resource-semantics.att;'                         >

<!ENTITY % a.loc.internal.ptr
         'target              IDREF                  #IMPLIED
          xpointer            CDATA                  #IMPLIED
          %locator;
          %locator.att;
          %remote-resource-semantics.att;'                        >

<!ENTITY % a.loc.external.ptr
         'entityref           ENTITY                 #IMPLIED
          xpointer            CDATA                  #IMPLIED
          %locator;
          %locator.att;
          %remote-resource-semantics.att;'                        >


<!--************************************************************-->
<!--          END LINKING AND POINTING ATTRIBUTES WITH XLINK    -->
<!--************************************************************-->



<!--************************************************************-->
<!--************************************************************-->
<!--          ENCODED ARCHIVAL DESCRIPTION                      -->
<!--          ELEMENT DECLARATIONS                              -->
<!--************************************************************-->
<!--************************************************************-->


<!ELEMENT ead                (eadheader, frontmatter?, archdesc)  >

<!ATTLIST ead
          %a.common;
          relatedencoding     CDATA                  #IMPLIED     >




<!--************************************************************-->
<!--           EAD HEADER                                       -->
<!--************************************************************-->


<!ELEMENT eadheader          (eadid, filedesc, profiledesc?,
                              revisiondesc?)                      >
<!ATTLIST eadheader
          %a.common;
          langencoding       CDATA                   #IMPLIED
          relatedencoding    CDATA                   #IMPLIED
          findaidstatus      (unverified-partial-draft |
                             unverified-full-draft |
                             edited-partial-draft |
                             edited-full-draft)      #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           EAD ID                                           -->

<!ELEMENT eadid              (#PCDATA)                            >

<!ATTLIST eadid
          systemid           CDATA                   #IMPLIED
          source             CDATA                   #IMPLIED
          type               CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           FILE DESCRIPTION ELEMENTS                        -->

<!ELEMENT filedesc           (titlestmt, editionstmt?,
                              publicationstmt?, seriesstmt?,
                              notestmt?)                          >

<!ATTLIST filedesc
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           TITLE STATEMENT                                  -->

<!ELEMENT titlestmt          (titleproper+, subtitle*, author?,
                              sponsor?)                           >

<!ATTLIST titlestmt
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           TITLE PROPER                                     -->

<!ELEMENT titleproper        (#PCDATA | %m.phrase.bare; | abbr |
                              date | expan | num)*                >

<!ATTLIST titleproper
          %a.common;
          extent             (all | part |
                              unknown-extent)        #IMPLIED
          pubstatus          (pub | unpub |
                              unknown-pub)           #IMPLIED
          render             (%av.render;)           #IMPLIED
          type               CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           SUBTITLE                                         -->

<!ELEMENT subtitle           (#PCDATA | %m.phrase.bare; | abbr |
                              date | expan | num)*                >

<!ATTLIST subtitle
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           AUTHOR                                           -->

<!ELEMENT author             (#PCDATA | %m.phrase.bare;)*         >

<!ATTLIST author
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           SPONSOR                                          -->

<!ELEMENT sponsor            (#PCDATA | %m.phrase.bare;)*         >

<!ATTLIST sponsor
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           EDITION STATEMENT                                -->

<!ELEMENT editionstmt        (edition | p)+                       >

<!ATTLIST editionstmt
          %a.common;
          encodinganalog      CDATA                  #IMPLIED     >


<!--************************************************************-->
<!--           PUBLICATION STATEMENT                            -->

<!ELEMENT publicationstmt
                              (publisher | date | address | num
                                  | p)+                           >

<!ATTLIST publicationstmt
          %a.common;
          encodinganalog     CDATA                  #IMPLIED      >


<!--************************************************************-->
<!--           SERIES STATEMENT                                 -->

<!ELEMENT seriesstmt         (titleproper | num | p)+             >

<!ATTLIST seriesstmt
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           NOTE STATEMENT                                   -->

<!ELEMENT notestmt           (note)+                              >

<!ATTLIST notestmt
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           PROFILE DESCRIPTION                              -->

<!ELEMENT profiledesc        (creation?, langusage?)              >

<!ATTLIST profiledesc
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           CREATION                                         -->

<!ELEMENT creation           (#PCDATA | %m.phrase.basic; | date)* >

<!ATTLIST creation
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           LANGUAGE USAGE                                   -->

<!ELEMENT langusage          (#PCDATA | %m.phrase.basic; |
