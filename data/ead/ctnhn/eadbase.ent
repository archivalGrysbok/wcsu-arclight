<!--************************************************************-->
<!--           DESCRIPTIVE IDENTIFICATION                       -->

<!ELEMENT did                (head?, (%m.did;)*)                  >

<!ATTLIST did
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           PHYSICAL DESCRIPTION                             -->

