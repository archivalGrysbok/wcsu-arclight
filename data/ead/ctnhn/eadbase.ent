<!--************************************************************-->
<!--          PARAMETER ENTITY DECLARATIONS                     -->
<!--************************************************************-->
<!--                                                            -->
<!-- Conventions:                                               -->
<!--              m.     content model components               -->
<!--              cm.    complete content model                 -->
<!--              a.     attribute model components             -->
<!--              av.    attribute declared value               -->
<!--                       name-token-groups                    -->
