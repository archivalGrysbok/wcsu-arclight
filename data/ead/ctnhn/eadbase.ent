<!--************************************************************-->
<!--           REPOSITORY                                       -->

<!ELEMENT container          (#PCDATA | %m.phrase.basic;)*        >

<!ATTLIST container
          %a.common;
          label              CDATA                   #IMPLIED
          type               (%av.container;)        #IMPLIED
          othertype          CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED
          parent             IDREFS                  #IMPLIED     >


<!--************************************************************-->
