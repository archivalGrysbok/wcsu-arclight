                              dimensions | physfacet | extent |
                              date | %m.access;)*                 >

<!ATTLIST physdesc
          %a.common;
          source             (%av.sources;)          #IMPLIED
          othersource        CDATA                   #IMPLIED
          label              CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           DIMENSIONS                                       -->

<!ELEMENT dimensions         (#PCDATA | %m.phrase.basic; |
                              dimensions)*                        >

<!ATTLIST dimensions
          %a.common;
          type               CDATA                   #IMPLIED
          unit               CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           PHYSICAL FACET                                   -->

<!ELEMENT physfacet          (#PCDATA | %m.phrase.basic; |
