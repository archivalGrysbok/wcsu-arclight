<!ATTLIST physfacet
          %a.common;
          type               CDATA                   #IMPLIED
          unit               CDATA                   #IMPLIED
          source             (%av.sources;)          #IMPLIED
          othersource        CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           EXTENT                                           -->

<!ELEMENT extent             (#PCDATA | %m.phrase.basic;)*        >

<!ATTLIST extent
          %a.common;
          type               CDATA                   #IMPLIED
          unit               CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           ORIGINATION                                      -->

<!ELEMENT origination        (#PCDATA | %m.phrase.basic;|
                              corpname | famname | name |
                              persname)*                          >

<!ATTLIST origination
          %a.common;
          label              CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           REPOSITORY                                       -->

<!ELEMENT repository         (#PCDATA | %m.phrase.basic; |
                              address | corpname | name |
                              subarea)*                           >

<!ATTLIST repository
          %a.common;
          label              CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           SUBAREA                                          -->

<!ELEMENT subarea            (#PCDATA | %m.phrase.bare;)*         >

<!ATTLIST subarea
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           UNIT DATE                                        -->

<!ELEMENT unitdate           (#PCDATA | %m.phrase.basic;)*        >

<!ATTLIST unitdate
          %a.common;
          label              CDATA                   #IMPLIED
          type               (bulk | inclusive |
                              single)                #IMPLIED
          certainty          CDATA                   #IMPLIED
          normal             CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           UNIT IDENTIFICATION                              -->

<!ELEMENT unitid             (#PCDATA | %m.phrase.basic;)*        >

<!ATTLIST unitid
          %a.common;
          label              CDATA                   #IMPLIED
          type               CDATA                   #IMPLIED
          encodinganalog     CDATA                   #IMPLIED
          countrycode        CDATA                   #IMPLIED
