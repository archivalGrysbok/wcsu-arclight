<!ATTLIST langusage
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           LANGUAGE (of description)                        -->

<!ELEMENT language           (#PCDATA | %m.phrase.bare;)*         >

<!ATTLIST language
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           REVISION DESCRIPTION                             -->


<!ELEMENT revisiondesc       (list | change+)                     >

<!ATTLIST revisiondesc
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >


<!--************************************************************-->
<!--           CHANGE                                           -->

<!ELEMENT change             (date, item+)                        >

<!ATTLIST change
          %a.common;
          encodinganalog     CDATA                   #IMPLIED     >





<!--************************************************************-->
<!--           FRONT MATTER ELEMENTS                            -->
<!--************************************************************-->


<!ELEMENT frontmatter         (titlepage?, div*)                  >

<!ATTLIST frontmatter
          %a.common;                                              >


<!--************************************************************-->
<!--           TITLE PAGE                                       -->

