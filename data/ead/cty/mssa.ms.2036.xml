         <c id="aspace_03e6940abca09bf7ab40d899b70e0adb" level="series">
            <did>
               <unittitle>Research materials</unittitle>
               <unitid>Series IV</unitid>
               
                  <unitdate type="inclusive" normal="1914/1981">1914-1981</unitdate>
               
