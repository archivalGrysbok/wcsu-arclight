   <archdesc level="collection">
      <did>
         <unittitle>Berkeley College, Yale University, photographs</unittitle>
         <unitid>RU 603</unitid>
         <repository>
            <corpname>
               Yale University. Department of Manuscripts and Archives
            </corpname>
         </repository>
         <langmaterial>
