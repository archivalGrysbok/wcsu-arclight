            </chronitem>
            <chronitem>
               <date>1894 Feb-May</date>
               
                  <event>In private boarding school in Hanover, Germany</event>
               
            </chronitem>
            <chronitem>
               <date>1894-1897</date>
               
                  <event>Attended, graduated from Newton High School, Newtonville, MA</event>
               
            </chronitem>
            <chronitem>
               <date>1897-1901 </date>
               
                  <event>B.A., Smith College</event>
               
            </chronitem>
