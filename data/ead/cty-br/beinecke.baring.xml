   <archdesc level="collection">
      <did>
         <unittitle>Maurice Baring collection</unittitle>
         <unitid>GEN MSS 322</unitid>
         <repository>
            <corpname>
               Beinecke Rare Book and Manuscript Library
            </corpname>
         </repository>
         <langmaterial>
