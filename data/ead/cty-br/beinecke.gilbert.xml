      <dsc>
         <c id="ref10" level="series">
            <did>
               <unittitle>I. Letters from W. S. Gilbert</unittitle>
               <unitdate type="inclusive">1908 May 29</unitdate>
            </did>
            <c id="ref11" level="file">
               <did>
                  <unittitle>ALS to Mr. Philip Carr, Grim's Dyke, Harrow Weald</unittitle>
                  <unitdate type="inclusive">1908 May 29</unitdate>
