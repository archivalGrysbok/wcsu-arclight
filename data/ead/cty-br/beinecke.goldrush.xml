         <c id="ref10" level="file">
            <did>
               <unittitle>Edmunds, A. C. and Cynthia</unittitle>
               <unitdate type="inclusive">1852-1860</unitdate>
               <container label="Mixed Materials (39002104796165)"
                          id="aspace_00f366ba43b22e6b53af3e06328c90be"
