            <c id="ref26" level="file">
               <did>
                  <unittitle>Political tracts and pamphlets</unittitle>
                  <unitdate type="inclusive">1968-1978, undated</unitdate>
                  <container label="Mixed Materials (39002099259724)"
                             id="aspace_5d44805717eefdf0a2675920774ff0a6"
